@model ASP.NetCoreCRUD.Models.DataModel.Student
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<form asp-action="edit" asp-controller="student" asp-route-id="@Model.IdStudent">
    <div class="form-group">
        <label asp-for="IdStudent" class="control-label"></label>
        <input type="text" asp-for="@Model.IdStudent" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="FirstName" class="control-label"></label>
        <input type="text" asp-for="FirstName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MiddleName" class="control-label"></label>
        <input type="text" asp-for="MiddleName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="LastName" class="control-label"></label>
        <input type="text" asp-for="LastName" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn-dark" />
    </div>
</form>
